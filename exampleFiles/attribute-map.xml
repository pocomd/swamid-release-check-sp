    <Attribute name="urn:mace:dir:attribute-def:eduPersonScopedAffiliation" id="saml_eduPersonScopedAffiliation">
        <AttributeDecoder xsi:type="ScopedAttributeDecoder" caseSensitive="false"/>
    </Attribute>

